FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          tetFemSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    motionU CG
    {
        preconditioner      Cholesky;

        minIter          0;
        maxIter          500;
        tolerance        1e-7;
        relTol           0.01;
    };
}


